{
    "source":{
      "include": [ "" ],
      "exclude": ["./node_modules","./build"],
      "includePattern": ".+\\.js(doc)?$",
      "excludePattern": "(^|\\/|\\\\)_"
    },
      "tags": {
        "allowUnknownTags": true
      },
      "plugins": ["plugins/markdown"],
      "templates": {
        "logoFile": "../resources/img/clinicaalemana.png",
        "cleverLinks": false,
        "monospaceLinks": false,
        "dateFormat": "ddd MMM Do YYYY",
        "outputSourceFiles": true,
        "outputSourcePath": true,
        "systemName": "CAS - Parrafos",
        "footer": "",
        "copyright": " Cas Copyright Â© 2020-2015 The contributors to the JSDoc3 and DocStrap projects.",
        "navType": "vertical",
        "theme":"flatly",
        "linenums": true,
        "collapseSymbols": false,
        "inverseNav": true,
        "protocol": "html://",
        "methodHeadingReturns": false
      },
      "markdown": {
        "parser": "gfm",
        "hardwrap": true
      },
      "opts":{
        "recurse":true,
        "destination": "./docs/"
      }
    }
    